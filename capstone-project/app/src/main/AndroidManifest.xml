<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bigbluebutton">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.bigbluebuttion.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.bigbluebuttion.about"></activity>
        <activity android:name="com.example.bigbluebuttion.Login.LoginFragment"></activity>
        <activity android:name="com.example.bigbluebuttion.Login.SignupFragment"></activity>
        <activity android:name="com.example.bigbluebuttion.forget_passwrod"></activity>
        <activity android:name="com.example.bigbluebuttion.home"></activity>
        <activity android:name="com.example.bigbluebuttion.setting"></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>